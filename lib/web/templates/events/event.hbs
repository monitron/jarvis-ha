{{#if historical}}
<div class="times">
  {{#if start}}<div class="start">{{start}}</div>{{/if}}
  {{#if end}}
  <div class="til"><i class="fa fa-chevron-down"></i></div>
  <div class="end">{{end}}</div>
  {{/if}}
</div>
{{/if}}

<div class="source">
  <i class="fa fa-{{sourceIcon}}"></i>
  <span class="name">{{sourceName}}</span>
</div>

<div class="title">{{title}}</div>
