<div class="section">
  <div class="title">
    {{title}}
  </div>
</div>
{{#each vehicles}}
<div class="section description">
  <div class="right">
    {{status}}
    {{#if batteryLevelText}}
    <span class="minor">&bull;</span>
    {{batteryLevelText}}
    <i class="fa {{batteryLevelIcon}}"></i>
    {{/if}}
  </div>
  <div class="normal-weight">{{name}}</div>
  <div class="minor">
    {{#if asOf}}<div class="right">as of {{asOf}}</div>{{/if}}
    <div>{{#if rangeText}}Range {{rangeText}}{{/if}}</div>
  </div>
  {{#if chargeDoneAt}}
  <div class="subsection"><i class="fa fa-bolt"></i> Charging will finish at about {{chargeDoneAt}}.</div>
  {{/if}}
</div>
{{/each}}


